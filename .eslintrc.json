{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "class-methods-use-this": "off",
    "camelcase": "off",
    "no-unused-vars": "warn",
    "operator-linebreak": "off",
    "arrow-parens": "off",
    "object-curly-newline": "off"
  }
}
